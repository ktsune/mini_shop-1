<h2>Edit a Merchant</h2>

<%= form_tag "/merchants/#{@merchant.id}", method: :patch do %>
  <%= label_tag :name %>
  <%= text_field_tag :name %>
  <%= value="name" %>
  <%= label_tag :address %>
  <%= text_field_tag :address %>
  <%= label_tag :city %>
  <%= text_field_tag :city %>
  <%= label_tag :state %>
  <%= text_field_tag :state %>
  <%= label_tag :zipcode %>
  <%= text_field_tag :zipcode %>

  <%= submit_tag 'Submit' %>
<% end %>
